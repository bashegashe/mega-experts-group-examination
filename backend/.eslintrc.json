{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "settings": {
        "import/resolver": {
            "webpack": {
                "config": "webpack.config.cjs"
            }
        }
    },
    "rules": {
        "import/prefer-default-export": "off",
        "import/no-extraneous-dependencies": "off",
        "no-console": "off",
        "consistent-return": "off",
        "no-param-reassign": "off",
        "max-classes-per-file": "off",
        "import/extensions": "off",
        "import/no-cycle": "off"
    }
}
